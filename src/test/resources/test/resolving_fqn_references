module Demo {
    entity TypeExamplesEntity {
        integerValue : integer = 10;
        stringValue : string = "Example string";
        unknownStringValue : string;
        implicitBoolean = false;
    }
    entity AnotherEntity {
        entityFeature : TypeExamplesEntity;
        stringValue = "Another example string";
        unknownTypeOrValue;
    }
    entity FqnResolution {
        fromExternalEntity = TypeExamplesEntity.integerValue;
        previouslyDeclaredEntity = fromExternalEntity;
        notDeclaredYetEntity = byModuleEntityFeature;
        byModuleEntityFeature = Demo.AnotherEntity.stringValue;
        stringValue = TypeExamplesEntity.stringValue;
        unknownTypeOrValue = AnotherEntity.unknownTypeOrValue;
        selfReference = selfReference;
        typedSelfReference : boolean = typedSelfReference;
        unsolvable = nonExistentReference;
        partiallyUnsolvable = Demo.AnotherNonExistentReference;
    }
}